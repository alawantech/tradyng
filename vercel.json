{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/dashboard/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/features",
      "dest": "/index.html"
    },
    {
      "src": "/pricing",
      "dest": "/index.html"
    },
    {
      "src": "/demo/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "pages/api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Subdomain-Support",
          "value": "enabled"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/subdomain-demo",
      "destination": "/demo/subdomains",
      "permanent": false
    }
  ]
}